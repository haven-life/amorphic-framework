{
    "name": "@haventech/supertype",
    "description": "A type system for classical inheritence, mix-ins and composition.",
    "version": "5.0.1",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "homepage": "https://github.com/haven-life/supertype",
    "dependencies": {
        "@haventech/amorphic-contracts": "^0.2.0",
        "nconf": "^0.12.0",
        "reflect-metadata": "^0.1.12"
    },
    "devDependencies": {
        "@types/chai": "4.3.0",
        "@types/mocha": "9.1.0",
        "@types/nconf": "^0.10.0",
        "@types/node": "16.11.22",
        "chai": "4.3.6",
        "eslint": "8.8.0",
        "mocha": "9.2.0",
        "mock-fs": "5.1.2",
        "nyc": "15.1.0",
        "sinon": "13.0.1",
        "ts-node": "10.5.0",
        "typescript": "3.9.10"
    },
    "scripts": {
        "lint": "eslint .",
        "debug:tests": "mocha --inspect-brk test/animals.js",
